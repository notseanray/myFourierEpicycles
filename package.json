{
  "name": "myfourierepicycles",
  "version": "1.0.0",
  "description": "Draw your own fourier epicycles.",
  "main": "index.js",
  "engines": {
    "node": "14.6.0"
  },
  "dependencies": {
    "dropzone": "^5.7.2",
    "mathjax": "^3.0.5",
    "p5": "^1.1.9",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "css-loader": "^4.2.1",
    "dot-prop": "^5.2.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "npm-watch": "^0.6.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "watch": {
    "webpack": "src/js/**/*.js",
    "css": "src/css/style.css"
  },
  "scripts": {
    "watch": "npm-watch",
    "webpack": "webpack || exit 1",
    "webpack-prod": "webpack --mode=production || exit 1",
    "css": "cp -r src/css dist/",
    "html": "cp src/index.html dist/",
    "build": "npm run webpack && npm run css && npm run html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trozler/myFourierEpicycles.git"
  },
  "author": "Tony Rosler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trozler/myFourierEpicycles/issues"
  },
  "homepage": "https://github.com/trozler/myFourierEpicycles#readme"
}
